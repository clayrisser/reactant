  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/reactant"
  },
  "scripts": {
    "build": "npm run test && babel src -d lib --extensions \".ts,.tsx\" --source-maps inline && tsc -d --emitDeclarationOnly && cp -r src/docker lib",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/* && jest --clearCache",
    "clean:purge": "jest --clearCache && git clean -fXd",
    "format": "prettier --write ./**/*.{json,md,scss,yaml,yml,js,jsx,ts,tsx} --ignore-path .gitignore",
    "lint": "npm run format && tsc --allowJs --noEmit && eslint --ext .ts,.tsx ./",
    "lint:fix": "npm run format && tsc --noEmit && eslint --ext .ts,.tsx --fix ./",
    "prepare": "sh prepare.sh",
    "prepublish": "npm run build",
    "start": "babel-node src --extensions \".ts,.tsx\"",
    "test": "npm run lint:fix && jest --coverage",
    "test:watch": "npm run lint && jest --watch"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@craco/craco": "^5.6.0",
    "@reactant/context": "^0.0.295",
    "@reactant/platform": "^0.0.295",
    "@storybook/react": "^5.2.8",
    "@types/webpack": "^4.41.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "circular-json": "^0.5.9",
    "core-js": "^3.6.1",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "peerDependencies": {
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "babel-loader": "^8.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@types/circular-json": "^0.4.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.25",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/webpack": "^4.41.0",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-typescript-to-proptypes": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-reactant": "^0.1.144",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testRegex": "tests(/|/.*/)[^_/]*[jt]sx?$",
