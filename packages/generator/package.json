{
  "name": "generator-reactant",
  "version": "0.0.389",
  "description": "yeoman generator for reactant",
  "keywords": [
    "yeoman-generator",
    "generator",
    "yo",
    "yeoman"
  ],
  "main": "generators/app/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "make -s build",
    "clean": "make -s clean",
    "coverage": "make -s coverage",
    "format": "make -s format",
    "inc": "npm version patch --git=false 2>/dev/null || true",
    "lint": "make -s lint",
    "make": "make --version",
    "prepare": "make -s prepare",
    "purge": "make -s purge",
    "start": "make -s start",
    "test": "make -s test",
    "test:ui": "make -s test-ui",
    "test:watch": "make -s test-watch"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "core-js": "^3.6.5",
    "create-react-app": "^3.4.1",
    "execa": "^4.0.0",
    "fs-extra": "^9.0.0",
    "generator-github-project": "^0.2.11",
    "idempotent-babel-polyfill": "^7.4.4",
    "lodash": "^4.17.15",
    "ora": "^4.0.3",
    "pkg-dir": "^4.2.0",
    "prettier": "^2.0.5",
    "yeoman-generator": "^4.9.0",
    "yo-base-prompts": "^0.1.8",
    "yo-option-or-prompt": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-jam": "^0.2.20",
    "jest-sonar-reporter": "^2.0.0",
    "yo": "^3.1.1"
  },
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "homepage": "https://github.com/codejamninja/generator-node-module-typescript",
  "license": "MIT",
  "repository": "https://github.com/codejamninja/generator-node-module-typescript",
  "jestSonar": {
    "reportPath": "node_modules/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "eslintIgnore": [
    "generators",
    "demo"
  ]
}
