  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "reactant",
    "hooks": {
      "init": "./lib/hooks/init"
    }
  },
  "jestSonar": {
    "reportPath": "node_modules/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
