    "@types/node": "^13.11.0",
    "@types/webpack": "^4.41.10",
    "cspell": "^4.0.55",
    "eslint": "^6.8.0",
    "eslint-config-reactant": "^0.1.144",
    "jest": "^25.2.7",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.0.4",
    "regenerator-runtime": "^0.13.5",
    "typescript": "^3.8.3"
