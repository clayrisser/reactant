    "@oclif/parser": "^3.8.4",
    "@reactant/config": "workspace:*",
    "@reactant/context": "workspace:*",
    "@reactant/core": "workspace:*",
    "@reactant/types": "workspace:*",
    "core-js": "^3.6.4",
    "fs-extra": "^9.0.0",
    "gnumake": "^0.1.0",
    "pkg-dir": "^4.2.0"
